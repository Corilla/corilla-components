{
  "name": "corilla-components",
  "version": "0.0.3",
  "description": "An enterprise-class React components for building experiences for Corilla apps",
  "homepage": "https://github.com/corilla/corilla-components#readme",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "lint": "eslint src && eslint docs && eslint examples && sass-lint --verbose",
    "build": "npm run clean && babel --quiet src/components --out-dir lib && cpy \"**/*.scss\" ../../lib/ --parents --cwd=src/components && node scripts/compile.js",
    "demo": "npm run build && node server.js examples/webpack.config.js",
    "release-patch": "npm run build && npm version patch",
    "release-minor": "npm run build && npm version minor",
    "release-major": "npm run build && npm version major",
    "test": "ava",
    "watch-test": "npm test -- --watch",
    "cover-test": "nyc npm test",
    "watch-cover": "nodemon --quiet --watch src --exec npm run cover-test -s",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/corilla/corilla-components"
  },
  "keywords": [
    "components",
    "corilla",
    "react",
    "react-component",
    "ui",
    "ui-kit",
    "ui-library"
  ],
  "author": "nicksp",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/corilla/corilla-components/issues"
  },
  "ava": {
    "babel": "inherit",
    "files": [
      "src/components/**/*.spec.js"
    ],
    "source": [
      "src/components/**/*.{js,jsx}",
      "!lib/**/*"
    ],
    "verbose": true,
    "require": [
      "./test-env.setup.js"
    ]
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text",
      "html"
    ]
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "browser-env": "^2.0.16",
    "cpy-cli": "^1.0.1",
    "css-loader": "^0.26.1",
    "enzyme": "^2.6.0",
    "eslint": "^3.11.1",
    "eslint-plugin-react": "^6.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "fs-extra": "^1.0.0",
    "html-webpack-plugin": "^2.24.1",
    "ignore-styles": "^5.0.1",
    "img-loader": "^1.3.1",
    "jsdom": "^9.8.3",
    "json-loader": "^0.5.4",
    "node-sass": "^3.13.0",
    "nodemon": "^1.11.0",
    "nyc": "^10.0.0",
    "postcss-loader": "^1.2.0",
    "react-addons-test-utils": "^15.4.1",
    "release-it": "^2.5.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "through2": "^2.0.3",
    "underscore.string": "^3.3.4",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2"
  },
  "dependencies": {
    "autoprefixer": "^6.5.3",
    "babel-polyfill": "^6.20.0",
    "babel-register": "^6.18.0",
    "babel-runtime": "^6.18.0",
    "classnames": "^2.2.5",
    "express": "^4.14.0",
    "markdown-it": "^8.2.1",
    "markdown-it-imsize": "^2.0.1",
    "markdown-it-sanitizer": "^0.4.3",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0"
  }
}
